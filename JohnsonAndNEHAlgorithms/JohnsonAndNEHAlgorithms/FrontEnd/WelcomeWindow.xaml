<Window x:Name="WelcomeWindowObj" x:Class="JohnsonAndNEHAlgorithms.FrontEnd.WelcomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JohnsonAndNEHAlgorithms.FrontEnd"
        mc:Ignorable="d"
        Title="WelcomeWindow" Height="450" Width="800" Closed="OnClosed_ShowFarewellMessage" WindowStartupLocation="CenterScreen">
    <Grid Background="#FFAC9F9F">
        <Label x:Name="WelcomeLabel" Content="Welcome to Flow Shop scheduling algorithms App!" HorizontalAlignment="Center" Height="75" Margin="15,10,5,0" VerticalAlignment="Top" Width="772" HorizontalContentAlignment="Center" FontSize="36" IsEnabled="False" FontFamily="WarHeliosCondCBold"/>
        <Button x:Name="StartButton" Content="Button" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Center" Width="240" Height="80" Background="#FF878DC7" Foreground="Black" BorderBrush="{x:Null}" FontFamily="WarHeliosCondCBold" FontSize="24" Click="LoadFile_OnClick"/>
    </Grid>
</Window>
